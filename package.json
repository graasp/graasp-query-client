    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitest/ui": "2.0.5",
    "date-fns": "3.6.0",
    "env-cmd": "10.1.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.1.5",
    "i18next": "23.14.0",
    "jsdom": "24.1.1",
    "mock-socket": "9.3.1",
    "nock": "13.5.5",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.10",
    "typescript": "5.5.4",
    "uuid": "10.0.0",
    "vitest": "2.0.5"
  },
  "peerDependencies": {
    "@graasp/sdk": "^4.0.0",
    "@graasp/translations": "*",
    "react": "^18.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "prepack": "yarn build",
    "prepare": "yarn prepack && yarn hooks:install",
    "prettier:check": "prettier --check {src,test}/**/*.{js,ts,tsx}",
    "prettier:write": "prettier --write {src,test}/**/*.{js,ts,tsx}",
    "pre-commit": "yarn check",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "test": "yarn vitest",
    "test:ci": "yarn vitest run",
    "lint": "eslint .",
    "post-commit": "git status",
    "type-check": "tsc --noEmit",
    "check": "yarn prettier:check && yarn type-check && yarn lint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "files": [
    "dist"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.4.0"
}
