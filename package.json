  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/compat": "1.2.5",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.15.0",
    "@graasp/sdk": "5.7.0",
    "@graasp/translations": "1.43.0",
